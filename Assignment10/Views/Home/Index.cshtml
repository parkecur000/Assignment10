@{
    ViewData["Title"] = "Home Page";
}
@model IndexViewModel

<div>
    <h1>@Model.TeamName</h1>
</div>
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">First Name</th>
                <th scope="col">Middle Init</th>
                <th scope="col">Last Name</th>
                <th scope="col">Address</th>
                <th scope="col">City</th>
                <th scope="col">State</th>
                <th scope="col">Zip Code</th>
                <th scope="col">Phone Number</th>
            </tr>
        </thead>
        <tbody>
            
                @foreach (var x in Model.Bowlers)
                {
                    <tr>
                        <td>@x.BowlerFirstName</td>
                        <td>@x.BowlerMiddleInit</td>
                        <td>@x.BowlerLastName</td>
                        <td>@x.BowlerAddress</td>
                        <td>@x.BowlerCity</td>
                        <td>@x.BowlerState</td>
                        <td>@x.BowlerZip</td>
                        <td>@x.BowlerPhoneNumber</td>
                    </tr>
                }
        </tbody>
    </table>

<div page-info="@Model.PageNumberingInfo", 
     page-url-teamName ="@Model.TeamName"></div>
